package be.pxl.h5.opdr4;

public class H5Opdr4c {

	public static void main(String[] args) {
		double a1 = 5;
		double b1 = 6;
		int opl1 = a1 + b1;
//		int opl1 = (int) (a1 + b1);
		System.out.println(opl1);

		short a2 = 5;
		short b2 = 6;
		short opl2 = a2 + b2;
//		short opl2 = (short) (a2 + b2);
		System.out.println(opl2);
	}
}